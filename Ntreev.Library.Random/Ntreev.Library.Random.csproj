<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Condition="'$(OS)' == 'Windows_NT'">
    <TargetFrameworks>net45;netcoreapp3.0;netcoreapp2.2;netstandard2.0</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Condition="'$(OS)' == 'Unix'">
    <TargetFrameworks>netcoreapp3.0;netcoreapp2.2;netstandard2.0</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>4.0.19187.1259</Version>
    <AssemblyVersion>4.0</AssemblyVersion>
    <FileVersion>4.0.19187.1259</FileVersion>
    <RepositoryType>git</RepositoryType>
    <Copyright>Copyright © Ntreev Soft 2018</Copyright>
    <Description>this is submodule of crema project</Description>
    <Company>NtreevSoft</Company>
    <Authors>s2quake</Authors>
    <RepositoryUrl>https://github.com/NtreevSoft/Ntreev.Library</RepositoryUrl>
    <PackageTags>Crema</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\words.txt" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\words.txt" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <ProjectReference Include="..\Ntreev.Library\Ntreev.Library.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
</Project>